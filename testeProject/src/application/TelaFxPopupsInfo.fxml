<StackPane id="pnlStackPaneTelaPopups"
	xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox fx:id="painelTelaPopups">
			<children>
				<HBox id="hboxHeaderTelaPopups">
					<children>
						<Button fx:id="botaoClose" onAction="#close"
							styleClass="btn_close">
							<HBox.margin>
								<Insets left="180.0" />
							</HBox.margin>
						</Button>
					</children>
					<VBox.margin>
						<Insets top="25.0" />
					</VBox.margin>
				</HBox>
				<FontIcon fx:id="iconTela" />
				<Label fx:id="labelInfo" />
				<HBox id="boxRodape">
					<VBox.margin>
						<Insets top="30.0" />
					</VBox.margin>
					<children>
						<Button id="botaoCancelar" onAction="#close"
							styleClass="btn" text="Fechar">
						</Button>
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</StackPane>
